# Smart Contract Environment Variables
# Copy this file to .env and fill in your actual values

# Private key for deployment (DO NOT COMMIT THIS FILE)
# Generate a new private key for production use
PRIVATE_KEY=your_private_key_here

# RPC URL for the blockchain network
# For Celo Sepolia testnet: https://alfajores-forno.celo-testnet.org
# For Celo mainnet: https://forno.celo.org
RPC_URL=https://alfajores-forno.celo-testnet.org

# Etherscan API key for contract verification (optional)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas settings
GAS_LIMIT=3000000
GAS_PRICE=20000000000

# Contract addresses (will be set automatically after deployment)
VAULT_CONTRACT_ADDRESS=
AAVE_POOL_ADDRESS=
CUSD_TOKEN_ADDRESS=
